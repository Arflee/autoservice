    "@types/next-auth": "^3.15.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vercel/postgres": "^0.8.0",
    "bcrypt": "^5.1.1",
    "clsx": "^2.1.0",
    "daisyui": "^4.9.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "env-cmd": "^10.1.0",
    "next": "14.1.4",
    "next-auth": "^4.24.7",
    "postcss": "^8",
    "prisma": "^5.12.1",
    "react": "^18",
    "react-calendar": "^4.8.0",
    "react-dom": "^18",
    "react-router-dom": "^6.23.1",
    "sharp": "0.33.0",
    "tailwindcss": "^3.3.0",
    "validator": "^13.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@testing-library/react": "^14.3.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/validator": "^13.11.9",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.6.0",
    "autoprefixer": "^10.0.1",
    "concurrently": "^8.2.2",
    "daisyui": "^4.9.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.1.4",
    "eslint-config-prettier": "^9.1.0",
    "jsdom": "^24.0.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typedoc": "^0.25.13",
