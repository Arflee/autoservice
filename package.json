{
  "name": "swi-autoservice",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others \"npm run server\" \"npm run local\"",
    "local": "env-cmd -f .env.dev next dev",
    "server": "docker-compose up autoservice-db",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "docs": "typedoc --entryPoints ./app/lib --out docs --entryPointStrategy expand --exclude ./components/"
  },
  "dependencies": {
    "@auth/core": "^0.29.0",
    "@auth/prisma-adapter": "^1.6.0",
    "@prisma/client": "^5.12.1",
